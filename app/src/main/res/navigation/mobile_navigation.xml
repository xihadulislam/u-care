<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.project.ucare.screens.main.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_emergencyContact"
        android:name="com.project.ucare.screens.main.emergencyContact.EmergencyContactFragment"
        android:label="@string/title_emergency_contact"
        tools:layout="@layout/fragment_emergency_contact" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.project.ucare.screens.main.settings.SettingsFragment"
        android:label="@string/title_settings"
        tools:layout="@layout/fragment_settings" />
